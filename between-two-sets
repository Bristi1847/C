#include <stdio.h>


int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}


int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    int a[n], b[m];

    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for (int i = 0; i < m; i++)
        scanf("%d", &b[i]);

    
    int L = a[0];
    for (int i = 1; i < n; i++)
        L = lcm(L, a[i]);

    
    int G = b[0];
    for (int i = 1; i < m; i++)
        G = gcd(G, b[i]);

    
    int count = 0;
    for (int x = L; x <= G; x += L) {
        if (G % x == 0)
            count++;
    }

    printf("%d\n", count);

    return 0;
}
