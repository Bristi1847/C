#include <stdio.h>
#include <stdlib.h>

int* gradingStudents(int grades_count, int* grades, int* result_count) {
    
    *result_count = grades_count;

    
    int* result = (int*) malloc(grades_count * sizeof(int));

    for (int i = 0; i < grades_count; i++) {
        int grade = grades[i];

        if (grade < 38) {
            result[i] = grade;  // failing grade, no rounding
        } else {
            int nextMultiple = ((grade / 5) + 1) * 5;
            if (nextMultiple - grade < 3)
                result[i] = nextMultiple;  // round up
            else
                result[i] = grade;         // keep original
        }
    }

    return result;
}

int main() {
    int n;
    scanf("%d", &n);

    int* grades = (int*) malloc(n * sizeof(int));

    for (int i = 0; i < n; i++) {
        scanf("%d", &grades[i]);
    }

    int result_count;
    int* result = gradingStudents(n, grades, &result_count);

    for (int i = 0; i < result_count; i++) {
        printf("%d\n", result[i]);
    }

    free(grades);
    free(result);

    return 0;
}
